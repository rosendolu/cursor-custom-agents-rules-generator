{
  "mcpServers": {
    "TAV2": {
      "command": "env",
      "args": [
        "TAVILY_API_KEY=YOUR-FREE-API-KEY-HERE",
        "npx",
        "-y",
        "tavily-mcp@0.1.3"
      ]
    },
    "api fox docs": {
      "command": "npx",
      "args": ["-y", "apifox-mcp-server@latest", "--project=<project-id>"],
      "env": {
        "APIFOX_ACCESS_TOKEN": "<access-token>"
      }
    }
  }
}
